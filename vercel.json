{
  "version": 2,
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/index",
      "dest": "/api/index.js"
    },
    
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ]
}
